{
  "root": true,
  "env": {
    "browser": true,
    "es6": true,
    "commonjs": true,
    "jquery": true
  },
  "eslint.workingDirectories": [{ "mode": "auto" }],
  "eslint.debug": true,
  "extends": "eslint:recommended",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": [
      "error",
      { "argsIgnorePattern": "^_", "varsIgnorePattern": "^_" }
    ]
  }
}
